{"ts":1360075589333,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<html>\r\n<head>\r\n    <meta charset=utf-8>\r\n    <title>jQuery Tutorial - Lesson 4</title>\r\n    \r\n    <link rel=\"stylesheet\" href=\"day.css\" />\r\n    \r\n    <style>\r\n    </style>\r\n</head>\r\n<body>\r\n    <h1>jQuery Events</h1>\r\n    \r\n    <button data-file=\"day\">Day</button>\r\n    <button data-file=\"night\">Night</button>\r\n\r\n    <script src=\"//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js\"></script>\r\n   \r\n    <script>\r\n        (function() {\r\n            var link = $('link');\r\n         \r\n            $('button').on('click', function() {\r\n                var $this = $(this),\r\n                    stylesheet = $this.data('file');\r\n    \r\n                link.attr('href', stylesheet + '.css');\r\n    \r\n                $this\r\n                    .siblings('button')\r\n                        .removeAttr('disabled')\r\n                        .end()\r\n                    .attr('disabled', 'disabled');\r\n             });\r\n        })();\r\n    </script>\r\n</body>\r\n</html>"]],"start1":0,"start2":0,"length1":0,"length2":963}]],"length":963}
